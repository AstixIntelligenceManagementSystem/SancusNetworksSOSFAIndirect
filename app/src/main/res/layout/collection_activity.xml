<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#eeeeee"
	android:orientation="vertical" >
	<RelativeLayout
		android:layout_height="46dp"
		android:layout_width="fill_parent"
		android:background="@drawable/custom_header_color"
		android:paddingLeft="16dp"



		android:id="@+id/rtv_hdr">

		<ImageView
			android:id="@+id/btn_bck"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/ic_back"
			android:layout_alignParentLeft="true"
			android:layout_centerVertical="true"/>
		<TextView
			android:id="@+id/order_detail"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="#FFFFFF"
			android:layout_toRightOf="@+id/backbutton"
			android:paddingLeft="10dp"
			android:layout_centerInParent="true"
			android:gravity="center_horizontal|left"

			android:textStyle="bold"
			android:shadowColor="#80FFFFFF"
			android:textSize="18sp"
			android:text="@string/PaymentCollection"/>

		<ImageView
			android:id="@+id/img_return"

			android:layout_toRightOf="@+id/order_detail"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true"
			android:layout_alignParentTop="true"
			android:src="@drawable/return_order"
			android:layout_marginLeft="30dp"
			android:visibility="gone" />

	</RelativeLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:background="@drawable/shadow_with_5dp"
		android:orientation="vertical">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">

			<TextView
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:text="@string/PrvAmtOutStnding"
				android:textColor="#000"
				android:textSize="11sp" />

			<TextView
				android:id="@+id/tvPrvsAmntOtstndng"
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_marginBottom="5dp"
				android:layout_marginTop="5dp"
				android:text="0.00"
				android:textColor="#000"
				android:textSize="11sp" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginBottom="10dp"
			android:background="#868686" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">

			<TextView
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:text="@string/crntInvoice"
				android:textColor="#000"
				android:textSize="11sp" />

			<TextView
				android:id="@+id/tvCrntInvc"
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_marginBottom="5dp"
				android:layout_marginTop="5dp"
				android:text="0.00"
				android:textColor="#000"
				android:textSize="11sp" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginBottom="10dp"
			android:background="#868686" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">

			<TextView
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:text="@string/TtlAmtOutStnding"
				android:textColor="#000"
				android:textSize="11sp" />

			<TextView
				android:id="@+id/tvTotalOutstndng"
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_marginBottom="5dp"
				android:layout_marginTop="5dp"
				android:text="0.00"
				android:textColor="#000"
				android:textSize="11sp" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginBottom="10dp"
			android:background="#868686" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">

			<TextView
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:text="@string/minAmountDue"
				android:textColor="#000"
				android:textSize="11sp" />

			<TextView
				android:id="@+id/tvminAmountDue"
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_marginBottom="5dp"
				android:layout_marginTop="5dp"
				android:text="NA"
				android:textColor="#000"
				android:textSize="11sp" />
		</LinearLayout>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#9E9E9E"
			android:gravity="center"
			android:layout_marginTop="5dp"
			android:orientation="horizontal">

			<TextView
				android:id="@+id/pymtModeTextView"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:layout_weight="1"
				android:focusable="true"
				android:focusableInTouchMode="true"
				android:gravity="center"
				android:text="@string/PaymentMode"
				android:textColor="#fff"
				android:textSize="10sp"
				android:textStyle="bold" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/AmountTextview"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="1dp"
				android:layout_marginRight="1dp"
				android:layout_weight="1"
				android:focusable="true"
				android:focusableInTouchMode="true"
				android:gravity="center"
				android:text="@string/Amount"
				android:textColor="#fff"
				android:textSize="10sp"
				android:textStyle="bold" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/chequeNoTextview"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1.3"
				android:focusable="true"
				android:focusableInTouchMode="true"
				android:gravity="center"
				android:text="@string/RefNochequeNoTrnNo"
				android:textColor="#fff"
				android:textSize="10sp"
				android:textStyle="bold" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/DateLabelTextview"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="1dp"
				android:layout_marginRight="1dp"
				android:layout_weight="1"
				android:focusable="true"
				android:focusableInTouchMode="true"
				android:gravity="center"
				android:text="@string/txtDate"
				android:textColor="#fff"
				android:textSize="10sp"
				android:textStyle="bold" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/BankLabelTextview"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="2dp"
				android:layout_weight="1.5"
				android:focusable="true"
				android:focusableInTouchMode="true"
				android:gravity="center"
				android:text="@string/Bank"
				android:textColor="#fff"
				android:textSize="10sp"
				android:textStyle="bold" />


		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:background="#868686" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="10dp"
			android:gravity="center"
			android:orientation="horizontal">

			<TextView
				android:id="@+id/paymentModeSpinnerFirst"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_margin="2dp"
				android:layout_weight="1"
				android:background="@drawable/spinner_background_with_rectangle"
				android:padding="5dp"
				android:text="@string/txtSelect"
				android:textColor="#000"
				android:textSize="10dp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<EditText
				android:id="@+id/amountEdittextFirst"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1"
				android:background="@drawable/edittex_with_white_background"
				android:gravity="center"
				android:inputType="numberDecimal"
				android:maxLength="10"
				android:paddingBottom="5dp"
				android:paddingTop="5dp"
				android:textColor="#000"
				android:textColorHint="#a4a4a4"
				android:textSize="10sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<EditText
				android:id="@+id/checqueNoEdittextFirst"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1.3"
				android:background="@drawable/edittext_with_gray_background"
				android:enabled="false"
				android:gravity="center"
				android:inputType="number"
				android:maxLength="15"
				android:paddingBottom="5dp"
				android:paddingTop="5dp"
				android:textColor="#000"
				android:textSize="10sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/dateTextViewFirst"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_margin="2dp"
				android:layout_weight="1"
				android:background="@drawable/outside_boundry_gray"
				android:gravity="center"
				android:text=""
				android:textColor="#000"
				android:textSize="9sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/BankSpinnerFirst"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_margin="2dp"
				android:layout_weight="1.5"
				android:background="@drawable/edittext_with_gray_background"
				android:enabled="false"
				android:padding="5dp"
				android:text=""
				android:textColor="#000"
				android:textSize="10dp" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:background="#868686" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="10dp"
			android:gravity="center"
			android:orientation="horizontal">

			<TextView
				android:id="@+id/paymentModeSpinnerSecond"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_margin="2dp"
				android:layout_weight="1"
				android:background="@drawable/edittex_with_white_background"
				android:padding="5dp"
				android:text="@string/txtSelect"
				android:textColor="#000"
				android:textSize="10dp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<EditText
				android:id="@+id/amountEdittextSecond"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1"
				android:background="@drawable/edittex_with_white_background"
				android:gravity="center"
				android:inputType="numberDecimal"
				android:maxLength="10"
				android:paddingBottom="5dp"
				android:paddingTop="5dp"

				android:textColor="#000"
				android:textSize="10sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<EditText
				android:id="@+id/checqueNoEdittextSecond"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1.3"
				android:background="@drawable/edittex_with_white_background"
				android:gravity="center"
				android:inputType="number"
				android:maxLength="15"
				android:paddingBottom="5dp"
				android:paddingTop="5dp"
				android:textColor="#000"
				android:textSize="10sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/dateTextViewSecond"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1"
				android:background="@drawable/outside_boundry_gray"
				android:drawableRight="@drawable/calander_quotation"
				android:gravity="center"
				android:text=""
				android:textColor="#000"
				android:textSize="9sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/BankSpinnerSecond"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_margin="2dp"
				android:layout_weight="1.5"
				android:background="@drawable/spinner_gray_background"
				android:padding="5dp"
				android:text="@string/txtSelect"
				android:textColor="#000"
				android:textSize="10dp" />
		</LinearLayout>


		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:background="#868686" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="10dp"
			android:gravity="center"
			android:orientation="horizontal">

			<TextView
				android:id="@+id/paymentModeSpinnerThird"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_margin="2dp"
				android:layout_weight="1"
				android:background="@drawable/edittex_with_white_background"
				android:padding="5dp"
				android:text="@string/txtSelect"
				android:textColor="#000"
				android:textSize="10dp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<EditText
				android:id="@+id/amountEdittextThird"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1"
				android:background="@drawable/edittex_with_white_background"
				android:gravity="center"
				android:inputType="numberDecimal"
				android:maxLength="10"
				android:paddingBottom="5dp"
				android:paddingTop="5dp"

				android:textColor="#000"
				android:textSize="10sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<EditText
				android:id="@+id/checqueNoEdittextThird"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1.3"
				android:background="@drawable/edittex_with_white_background"
				android:gravity="center"
				android:inputType="number"
				android:maxLength="15"
				android:paddingBottom="5dp"
				android:paddingTop="5dp"
				android:textColor="#000"
				android:textSize="10sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/dateTextViewThird"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_margin="1dp"
				android:layout_weight="1"
				android:background="@drawable/outside_boundry_gray"
				android:drawableRight="@drawable/calander_quotation"
				android:gravity="center"
				android:text=""
				android:textColor="#000"
				android:textSize="9sp" />

			<View
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="#868686" />

			<TextView
				android:id="@+id/BankSpinnerThird"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:layout_margin="2dp"
				android:layout_weight="1.5"
				android:background="@drawable/spinner_gray_background"
				android:padding="5dp"
				android:text="@string/txtSelect"
				android:textColor="#000"
				android:textSize="10dp" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginBottom="10dp"
			android:background="#868686" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">

			<TextView
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:text="@string/TotalAmount"
				android:textColor="#000"
				android:textSize="11sp" />

			<TextView
				android:id="@+id/totaltextview"
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:layout_marginBottom="5dp"
				android:layout_marginTop="5dp"
				android:text="0.00"
				android:textColor="#000"
				android:textSize="11sp" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginBottom="10dp"
			android:background="#868686" />



		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="33dp"
			android:layout_marginBottom="20dp"
			android:layout_marginTop="10dp"
			android:orientation="horizontal"
			android:visibility="visible">

			<Button
				android:id="@+id/Done_btn"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="20dp"

				android:layout_marginRight="20dp"
				android:layout_weight="1"
				android:background="@drawable/custom_button"
				android:text="@string/DoneAndExit"
				android:textColor="#fff"
				android:textSize="11sp"
				android:textStyle="bold" />

			<Button
				android:id="@+id/cancleBtn"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:layout_marginLeft="20dp"
				android:layout_marginRight="20dp"
				android:layout_weight="1"
				android:background="@drawable/custom_button"
				android:text="Cancel"
				android:textColor="#fff"
				android:textSize="11sp"
				android:textStyle="bold"
				android:visibility="gone" />


		</LinearLayout>

	</LinearLayout>


</LinearLayout>
