<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".LastVisitDetails"
	android:background="#ffffff"

	android:orientation="vertical"
	>
	<!-- android:background="#5B9BD5" -->
	<ScrollView android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="10">
		<RelativeLayout android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/parentOfWholePage">

			<TextView
				android:id="@+id/txt_storeSummary"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:text="Anand Store Summary "
				android:textSize="16sp"
				android:layout_marginTop="10dp"
				android:layout_alignParentLeft="true"
				android:textStyle="bold" />

			<!-- Visit Row Views Starts-->
			<TextView
				android:id="@+id/txt_visitDate"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:text="@string/LastVisitDate"
				android:textSize="12sp"
				android:layout_marginLeft="15dp"
				android:layout_below="@+id/txt_storeSummary"
				android:textStyle="bold" />

			<TextView
				android:id="@+id/txt_visitDate_Value"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBaseline="@+id/txt_visitDate"
				android:layout_toRightOf="@+id/txt_visitDate"
				android:textSize="12sp"
				android:text="10-06-2015"
				android:layout_marginLeft="25dp"
				android:textStyle="bold" />

			<Button
				android:id="@+id/txt_visitDate_Details"
				android:layout_width="wrap_content"
				android:layout_height="35dp"
				android:layout_alignBaseline="@+id/txt_visitDate_Value"
				android:layout_below="@+id/txt_visitDate_Value"
				android:layout_marginLeft="65dp"
				android:layout_marginTop="50dp"
				android:layout_toRightOf="@+id/txt_visitDate_Value"
				android:background="@drawable/custom_button_new"
				android:text="@string/Details"
				android:textColor="#0563C1"
				android:textSize="12sp"
				android:textStyle="bold"/>

			<!-- Visit Row Views Ends-->

			<!-- Orders Row Views Starts-->

			<TextView
				android:id="@+id/txt_orderDate"
				android:layout_width="100dp"
				android:layout_height="wrap_content"
				android:layout_below="@+id/txt_visitDate"
				android:text="@string/LastOrderDate"
				android:textSize="12sp"
				android:layout_marginLeft="15dp"
				android:layout_alignParentLeft="true"
				android:layout_marginTop="20dp"
				android:textStyle="bold"
				/>

			<TextView
				android:id="@+id/txt_orderDate_Value"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBaseline="@+id/txt_orderDate"
				android:layout_toRightOf="@+id/txt_orderDate"
				android:textSize="12sp"
				android:text="11-06-2015"
				android:layout_below="@+id/txt_visitDate_Value"
				android:layout_alignLeft="@+id/txt_visitDate_Value"
				android:textStyle="bold"
				/>

			<Button
				android:id="@+id/txt_orderDate_Details"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBaseline="@+id/txt_orderDate_Value"
				android:layout_toRightOf="@+id/txt_orderDate_Value"
				android:textSize="12sp"
				android:text="@string/Details"
				android:visibility="gone"
				android:background="#ED7D31"
				android:textColor="#0563C1"
				android:layout_below="@+id/txt_visitDate_Details"
				android:layout_alignLeft="@+id/txt_visitDate_Details"
				android:textStyle="bold"  />

			<!-- Order Row Views Ends-->


			<!-- Execution Row Views Starts-->

			<TextView
				android:id="@+id/txt_execution"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_below="@+id/txt_orderDate"
				android:text="@string/ExecutionSummary"
				android:textSize="12sp"
				android:layout_marginLeft="15dp"

				android:layout_alignParentLeft="true"
				android:layout_marginTop="20dp"
				android:textStyle="bold"
				/>

			<TextView
				android:id="@+id/txt_execution_Value"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBaseline="@+id/txt_execution"
				android:layout_toRightOf="@+id/txt_execution"
				android:textSize="12sp"
				android:text="11-06-2015"

				android:layout_below="@+id/txt_orderDate_Value"
				android:layout_alignLeft="@+id/txt_orderDate_Value"
				android:textStyle="bold"
				android:visibility="gone"
				/>

			<Button
				android:id="@+id/txt_execution_Details"
				android:layout_width="wrap_content"
				android:layout_height="35dp"
				android:layout_alignBaseline="@+id/txt_execution_Value"
				android:layout_alignLeft="@+id/txt_orderDate_Details"
				android:layout_below="@+id/txt_orderDate_Details"
				android:layout_toRightOf="@+id/txt_execution_Value"
				android:background="@drawable/custom_button_new"
				android:text="@string/Details"
				android:textColor="#0563C1"
				android:textSize="12dp"
				android:textStyle="bold"/>

			<!-- Execution Row Views Ends-->

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:id="@+id/ll_gstDetails"

				android:layout_below="@+id/parentOfAllPaymentStructure"
				android:layout_marginTop="10dp"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp">

				<TextView
					android:id="@+id/gstdetails"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="2dp"
					android:textStyle="bold"
					android:layout_marginLeft="10dp"
					android:layout_marginTop="10dp"
					android:text="@string/GSTDetails"

					android:textSize="13sp"/>

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3dp"
					android:layout_marginRight="3dp"
					android:layout_marginTop="5dp"
					android:layout_marginBottom="10dp"
					android:background="@drawable/my_border_seprate"
					android:orientation="vertical"
					android:id="@+id/ll_forGst"
					android:padding="8dp" >

					<TextView
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="2dp"
						android:text="@string/GSTCompliance"
						android:textColor="#1565C0"
						android:id="@+id/gst_compliance_txt"/>"

					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:gravity="center_horizontal">

						<RadioButton
							android:id="@+id/rb_gst_yes"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/AlertDialogYesButton"
							android:textColor="#000000"

							android:checked="false"/>


						<RadioButton
							android:id="@+id/rb_gst_no"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/NotRequired"
							android:textColor="#000000"

							android:checked="false"/>
						<RadioButton
							android:id="@+id/rb_pending"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Pending"
							android:textColor="#000000"
							android:checked="false"/>

					</LinearLayout>


					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:id="@+id/ll_gstDependent"
						android:visibility="gone"
						android:layout_marginTop="1dp">


						<TextView
							android:layout_width="0dp"
							android:layout_weight="0.60"
							android:layout_height="wrap_content"
							android:text="@string/GSTNo"
							android:textColor="#1565C0"
							android:paddingTop="2dp"
							android:paddingBottom="2dp"
							android:paddingLeft="2dp"
							android:id="@+id/text_gstYes" />

						<EditText
							android:id="@+id/edit_gstYes"
							android:layout_width="0dp"
							android:layout_weight="0.90"
							android:layout_height="wrap_content"
							android:background="@drawable/edittxt_border"
							android:maxLength="15"
							android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
							android:padding="4dp"
							android:enabled="false"
							android:textSize="12sp"

							android:textColor="#000000"/>

						<Button
							android:id="@+id/gst_Details_but"
							android:layout_width="0dp"
							android:layout_weight="0.50"
							android:layout_height="30dp"
							android:textSize="12sp"
							android:text="@string/Edit"
							android:paddingTop="5dp"
							android:paddingBottom="5dp"
							android:layout_marginLeft="20dp"
							android:layout_marginRight="20dp"
							android:background="@drawable/custom_button_new"
							android:textColor="#ffffff"
							android:textStyle="bold"
							android:visibility="gone"/>

					</LinearLayout>

				</LinearLayout>


			</LinearLayout>



			<!-- General Scheme Row Views Starts-->

			<TextView
				android:id="@+id/txt_generalScheme"
				android:text="@string/GeneralScheme"
				android:textSize="12sp"
				android:gravity="left"
				android:layout_width="wrap_content"
				android:textStyle="bold"
				android:layout_marginLeft="15dp"

				android:layout_alignParentLeft="true"
				android:layout_marginTop="20dp"
				android:layout_below="@+id/ll_InvoiceLastVisit"
				android:layout_height="wrap_content" />

			<TextView
				android:id="@+id/txt_GeneralScheme_Value"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBaseline="@+id/txt_generalScheme"
				android:layout_toRightOf="@+id/txt_generalScheme"
				android:textSize="12sp"
				android:text="@string/None"

				android:layout_marginLeft="30dp"
				android:textStyle="bold"

				/>


			<RelativeLayout
				android:id="@+id/relLayout_for_generalScheme"
				android:layout_width="match_parent"
				android:layout_height="200dp"
				android:layout_below="@+id/txt_generalScheme"
				android:layout_centerInParent="true"
				android:layout_marginTop="0dp"
				android:background="@drawable/my_border"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"  >

				<TableLayout
					android:id="@+id/tabLayoutHeader2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="#000000"
					android:stretchColumns="*" >

					<TableRow
						android:id="@+id/trHeader2"
						android:layout_margin="0.2dp" >

						<TextView
							android:layout_margin="0.2dp"
							android:background="#d8e2ee"
							android:scrollHorizontally="true"
							android:layout_width="wrap_content"
							android:text="@string/lastvisitdetails_schemedescription"
							android:textSize="10sp"
							android:gravity="left"
							android:textStyle="bold" />

					</TableRow>
				</TableLayout>

				<ScrollView
					android:id="@+id/verticalScrollView12"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/tabLayoutHeader2"
					android:layout_centerInParent="true"
					android:layout_marginLeft="0dp"
					>

					<TableLayout
						android:id="@+id/dyntable_SchemeApplicable"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="#000000"
						android:stretchColumns="*" >

					</TableLayout>
				</ScrollView>



			</RelativeLayout>

			<!-- General Scheme Row Views Ends-->

			<!-- Special Scheme Row Views Starts-->

			<TextView
				android:id="@+id/txt_specialScheme"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_below="@+id/relLayout_for_generalScheme"
				android:text="@string/SpecialScheme"
				android:textSize="12sp"
				android:layout_marginLeft="15dp"

				android:layout_alignParentLeft="true"
				android:layout_marginTop="15dp"
				android:textStyle="bold"
				/>


			<!-- Special Scheme Row Views Ends-->

			<TextView
				android:id="@+id/txt_specialScheme_Value"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBaseline="@+id/txt_specialScheme"
				android:layout_toRightOf="@+id/txt_specialScheme"
				android:textSize="12sp"
				android:text="@string/None"

				android:layout_marginLeft="30dp"
				android:textStyle="bold"

				/>


			<RelativeLayout
				android:id="@+id/relLayout_for_specialScheme"
				android:layout_width="match_parent"
				android:layout_height="100dp"
				android:layout_below="@+id/txt_specialScheme"
				android:layout_alignLeft="@+id/txt_specialScheme"


				android:background="@drawable/my_border"

				android:layout_marginRight="10dp"  >

				<TableLayout
					android:id="@+id/tabLayoutHeader2_specialScheme"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="#000000"
					android:stretchColumns="*" >

					<TableRow
						android:id="@+id/trHeader2_specialScheme"
						android:layout_margin="0.2dp" >

						<TextView
							android:layout_margin="0.2dp"
							android:background="#d8e2ee"
							android:scrollHorizontally="true"
							android:layout_width="wrap_content"
							android:text="@string/SpecialSchemeDescription"
							android:textSize="10sp"
							android:gravity="left"
							android:textStyle="bold" />

					</TableRow>
				</TableLayout>

				<ScrollView
					android:id="@+id/verticalScrollView12_specialScheme"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/tabLayoutHeader2_specialScheme"
					android:layout_centerInParent="true"
					android:layout_marginLeft="0dp" >

					<TableLayout
						android:id="@+id/dyntable_SpecialSchemeApplicable"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="#000000"
						android:stretchColumns="*" >

					</TableLayout>
				</ScrollView>



			</RelativeLayout>


			<!-- Special Scheme Row Views Ends-->


			<TextView
				android:id="@+id/txt_Quatation"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_below="@+id/relLayout_for_specialScheme"
				android:text="@string/QuotationInfo"
				android:textSize="12sp"
				android:layout_marginLeft="15dp"

				android:layout_alignParentLeft="true"
				android:layout_marginTop="15dp"
				android:textStyle="bold"

				android:visibility="gone"
				/>


			<!-- Special Scheme Row Views Ends-->

			<TextView
				android:id="@+id/txt_Quatation_Value"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignBaseline="@+id/txt_Quatation"
				android:layout_toRightOf="@+id/txt_Quatation"
				android:textSize="12sp"
				android:text="@string/None"

				android:layout_marginLeft="40dp"
				android:textStyle="bold"

				android:visibility="gone"
				/>

			<RelativeLayout
				android:id="@+id/relLayout_for_Quatation"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_below="@+id/txt_Quatation"
				android:layout_alignLeft="@+id/txt_Quatation"

				android:visibility="gone"
				android:background="@drawable/my_border"

				android:layout_marginRight="10dp"  >

				<TableLayout
					android:id="@+id/tabLayoutHeader2_Quatation"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="#000000"
					android:stretchColumns="*" >

					<TableRow
						android:id="@+id/trHeader2_Quatation"
						android:layout_margin="0.2dp"
						android:weightSum="10" >

						<TextView
							android:layout_margin="0.2dp"
							android:background="#d8e2ee"
							android:scrollHorizontally="true"
							android:layout_width="0dp"
							android:text="@string/QuotationCode"
							android:textSize="10sp"
							android:textStyle="bold"
							android:layout_weight="6"
							android:layout_marginLeft="10dp"/>

						<TextView
							android:layout_margin="0.2dp"
							android:background="#d8e2ee"
							android:scrollHorizontally="true"
							android:layout_width="0dp"
							android:text="@string/QuotStatus"
							android:textSize="10sp"
							android:textStyle="bold"
							android:gravity="center_horizontal"
							android:layout_weight="2" />

						<TextView
							android:layout_margin="0.2dp"
							android:background="#d8e2ee"
							android:scrollHorizontally="true"
							android:layout_width="0dp"
							android:text="@string/QuotDate"
							android:textSize="10sp"
							android:textStyle="bold"
							android:gravity="center_horizontal"
							android:layout_weight="2"  />

					</TableRow>
				</TableLayout>

				<ScrollView
					android:id="@+id/QuatationListScroll"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/tabLayoutHeader2_Quatation"

					android:layout_marginLeft="0dp" >


					<TableLayout
						android:id="@+id/dynprodtableQuatation"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="#000000"
						android:stretchColumns="*" >

					</TableLayout>


				</ScrollView>


			</RelativeLayout>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_below="@+id/relLayout_for_Quatation"
				android:id="@+id/ll"
				android:orientation="vertical"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				>
				<Button android:layout_width="wrap_content"
					android:layout_height="30dp"
					android:text="@string/NewQuotation"
					android:textSize="10sp"
					android:layout_gravity="center_horizontal"
					android:textColor="#c25"
					android:id="@+id/quotationBtn"/>
			</LinearLayout>
			<TextView
				android:id="@+id/paymntTxtview"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:text="@string/PaymentInfo"
				android:textSize="12sp"
				android:layout_marginLeft="15dp"
				android:visibility="gone"
				android:layout_below="@+id/ll"
				android:textStyle="bold" />
			<LinearLayout android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:layout_marginTop="4dp"
				android:background="@drawable/shadow_with_5dp"
				android:layout_below="@+id/paymntTxtview"
				android:visibility="gone"
				android:id="@+id/parentOfAllPaymentStructure">

				<LinearLayout android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#90544f88"
					android:gravity="center"

					android:orientation="horizontal">
					<TextView android:layout_width="0dp"
						android:text="@string/PaymentStage"
						android:gravity="center"
						android:id="@+id/paymentstagetextviewNew"
						android:focusable="true"
						android:focusableInTouchMode="true"
						android:textColor="#fff"
						android:layout_height="wrap_content"
						android:layout_weight="2"/>
					<View android:layout_width="1dp"
						android:background="#000"
						android:layout_height="match_parent"/>
					<TextView android:layout_width="0dp"
						android:text="@string/Percage"
						android:textColor="#fff"
						android:focusable="true"
						android:visibility="gone"
						android:focusableInTouchMode="true"
						android:id="@+id/percentageTextviewNew"
						android:gravity="center"
						android:layout_marginLeft="1dp"
						android:layout_marginRight="1dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"/>
					<View android:layout_width="1dp"
						android:background="#000"
						android:visibility="gone"
						android:layout_height="match_parent"/>
					<TextView android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:gravity="center"
						android:text="@string/PaymentMode"
						android:focusable="true"
						android:focusableInTouchMode="true"
						android:id="@+id/paymentModeTextviewNew"
						android:textColor="#fff"
						android:layout_weight="2"/>
					<View android:layout_width="1dp"
						android:background="#000"
						android:layout_height="match_parent"/>
					<TextView android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:text="@string/CreditDays"
						android:textColor="#fff"
						android:focusable="true"
						android:focusableInTouchMode="true"
						android:id="@+id/creditdaysTextboxNew"
						android:gravity="center"
						android:layout_marginLeft="1dp"
						android:layout_marginRight="1dp"
						android:layout_weight="1"/>
					<View android:layout_width="1dp"
						android:background="#000"
						android:layout_height="match_parent"/>
					<TextView android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:gravity="center"
						android:id="@+id/CreditlimitTextboxNew"
						android:textColor="#fff"
						android:focusable="true"
						android:focusableInTouchMode="true"
						android:layout_marginLeft="1dp"
						android:layout_marginRight="1dp"
						android:text="@string/CreditLimit"
						android:layout_weight="1"/>


				</LinearLayout>
				<View android:layout_width="match_parent"
					android:layout_height="1dp"
					android:background="#000"/>
				<LinearLayout android:layout_width="match_parent"
					android:orientation="horizontal"
					android:gravity="center"
					android:layout_height="wrap_content">
					<CheckBox android:layout_width="0dp"
						android:text="@string/AdvanceBeforeDelivery"
						android:textSize="10sp"
						android:layout_height="wrap_content"
						android:id="@+id/AdvanceBeforeDeliveryCheckBoxNew"
						android:layout_gravity="center"
						android:layout_weight="2"/>
					<View android:layout_width="1dp"
						android:background="#000"
						android:layout_height="match_parent"/>
					<EditText android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:enabled="false"
						android:textSize="14sp"
						android:visibility="gone"
						android:id="@+id/percentageOfAdvanceBeforeDelivery"
						android:background="@drawable/edittext_with_nocolor_change"
						android:gravity="center"
						android:inputType="number"
						android:layout_margin="1dp"
						android:layout_weight="1"/>
					<View android:layout_width="1dp"
						android:visibility="gone"
						android:background="#000"
						android:layout_height="match_parent"/>
					<LinearLayout
						android:layout_width="0dp"
						android:layout_height="wrap_content"

						android:id="@+id/parentOfAdvanceBeforeDeliveryPayMentMode"
						android:orientation="vertical"
						android:layout_weight="2" >


					</LinearLayout>
					<View android:layout_width="1dp"
						android:background="#000"
						android:layout_height="match_parent"/>
					<EditText android:layout_width="0dp"
